<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/vg_symptom_container"
    android:layout_marginTop="2dp"
    android:layout_marginBottom="2dp"
    android:background="@drawable/bg_white_rounded">

    <ImageView
        android:id="@+id/iv_symptom"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_margin="10dp"
        android:src="@drawable/ic_cough"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ContentDescription" />

    <TextView
        android:id="@+id/tv_symptom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="21dp"
        android:textColor="@color/general_text_color"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/iv_symptom"
        app:layout_constraintLeft_toRightOf="@+id/iv_symptom"
        app:layout_constraintTop_toTopOf="@+id/iv_symptom"
        tools:text="Tetsxdfsdsdn" />

</androidx.constraintlayout.widget.ConstraintLayout>